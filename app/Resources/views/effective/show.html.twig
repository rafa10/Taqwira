{% block body %}

    <!-- Modal Structure -->
    <div class="modal-content grey lighten-4">
        <h5 class="flow-text"><i class="medium left mdi-action-info"></i> informations effectif académie</h5><br>
        {#<div class="divider"></div><br>#}
        <div class="row">
            <div class="col s12"><strong>Nom & Prénom : {{ effective.sexe }}</strong>{{ effective.firstname ~ effective.lastname }}</div>
            <div class="col s12"><strong>Date de naissance : </strong> {{ effective.dob|date('d/m/Y') }}</div>
            <div class="col s12"><strong>Email : </strong> {{ effective.email }}</div>
            <div class="col s12"><strong>Téléphone : </strong> {{ effective.phone }}</div>
            <div class="col s12"><strong>Adresse : </strong> {{ effective.address }}</div>
            <div class="col s12"><strong>Poste : </strong> {{ effective.function }}</div>
            <div class="col s12"><strong>Poids : </strong> {{ effective.weight }}</div>
            <div class="col s12"><strong>Taille : </strong> {{ effective.size }}</div>
            <div class="col s12"><strong>Niveau scolaire : </strong> {{ effective.schoolLevel }}</div>
            <div class="col s12"><strong>Etablissement scolaire : </strong> {{ effective.educationalEstablishment }}</div>
            <div class="col s12"><strong>Date d'inscription : </strong> <span class="chip green green-text text-lighten-4">{{ effective.created|date('d/m/Y') }}</span></div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="modal-action modal-close waves-effect waves-green btn-flat">Fermer</button>
    </div>

{% endblock %}
