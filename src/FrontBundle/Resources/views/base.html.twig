<body>

<!-- Start Page Loading -->
<div id="loader-wrapper">
    <div id="loader"></div>
    <div class="loader-section section-left"></div>
    <div class="loader-section section-right"></div>
</div>
<!-- === End Page Loading === -->

<!-- //////////////////////////////////////////////////////////////////////////// -->
<!-- START HEADER -->
<header id="header">
    <!-- start header nav-->
    <div class="navbar-fixed">
        <nav>
            <div class="nav-wrapper grey darken-3">
                <a href="{{ path('home') }}" class="brand-logo darken-1" style="margin-top: 0.65rem"><img src="{{ asset('public/images/logo-takwira.png') }}" alt="Taqwira logo"></a>
                <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
                <ul class="right hide-on-med-and-down">
                    {% set vidLink = null %}{% set contLink = null %}{% set bilLink = null %}{% set btnLink = null %}{% set homLink = null %}{% set plaLink = 'hide' %}
                    {% if app.request.attributes.get('_route') == 'home' %}{% set homLink = 'hide' %}{% set plaLink = null %}{% endif %}
                    <li class="{{ homLink }}"><a href="{{ path('home') }}"></i>Accueil</a></li>
                    <li class="{{ plaLink }}"><a href="#plateforme"></i>Plateforme</a></li>
                    <li class="{{ contLink }}"><a href="#contact">Contact</a></li>
                    <li class="{{ vidLink }}"><a href="{{ path('video_login') }}">Match en vidéo</a></li>
                    <li class="{{ bilLink }}"><a href="{{ path('booking_login') }}">Mes billets</a></li>
                    <li class="{{ btnLink }}" style="margin-right: 0.5rem"><a class="btn waves-effect waves-light yellow accent-4 black-text" href="{{ path('fos_user_security_login') }}">S'identifier</a></li>
                </ul>
                <ul class="side-nav" id="mobile-demo">
                    {% set vidLink = null %}{% set bilLink = null %}{% set btnLink = null %}{% set homLink = null %}{% set plaLink = 'hide' %}
                    {% if app.request.attributes.get('_route') == 'home' %}{% set homLink = 'hide' %}{% set plaLink = null %}{% endif %}
                    <li class="{{ homLink }}"><a href="{{ path('home') }}" class="left"> <i class="left material-icons">home</i>Accueil</a></li>
                    <li class="{{ plaLink }}"><a href="#plateforme" class="left"><i class="left material-icons">apps</i> Plateforme</a></li>
                    <li class="{{ contLink }}"><a href="#contact" class="left"><i class="left material-icons">contact_mail</i>Contact</a></li>
                    <li class="{{ vidLink }}"><a href="{{ path('video_login') }}" class="left"><i class="left material-icons">ondemand_video</i> Match en vidéo</a></li>
                    <li class="{{ bilLink }}"><a href="{{ path('booking_login') }}" class="left"><i class="left material-icons">receipt</i>Mes billets</a></li>
                </ul>
            </div>
        </nav>
    </div>
    <!-- end header nav-->
</header>
<!-- END HEADER -->
<!-- //////////////////////////////////////////////////////////////////////////// -->

{% block body %}
    <!-- START MAIN -->
    <!-- END MAIN -->
{% endblock body %}

<!-- //////////////////////////////////////////////////////////////////////////// -->
<!-- START FOOTER -->
